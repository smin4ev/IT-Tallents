package Geometria;

public class Romb extends IzpuknalChetiriugalnik{

	double height;
	//getter for height
	double getHeight(){
		return this.height;
	}
	// setter for height
	void setHeight(double height){
		if(height>0){
			this.height = height;
		}
	}
	
	Romb(double a, double b, double c, double d) {
		super(a, b, c, d);
	
	}

	@Override
	public double calcSurface() {
		return this.getA()*this.getHeight();
	}

}
