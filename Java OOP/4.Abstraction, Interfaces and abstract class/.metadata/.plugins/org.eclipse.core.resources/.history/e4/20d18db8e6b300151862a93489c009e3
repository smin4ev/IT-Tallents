package Geometria;

public class Pravougulnik  extends Kvadrat{

	 Pravougulnik(double a, double b) {
		super(a);
	}
	 
	@Override
	public double calcSurface() {
		return this.getA()*this.getB();
	}
	
	public double calcPerimetar(double a, double b){
		this.setA(a);
		this.setB(b);
		return 2*this.getA() + 2*this.getB();
	}
}
